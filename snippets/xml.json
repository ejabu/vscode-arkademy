		],
	},
	"Xpath - Inside": {
		"prefix": "Xpath - Inside",
		"body": [
			"<xpath expr=\"//notebook\" position=\"inside\">",
			"	<page string=\"Other Info\">",
			"		<field name=\"is_part_time\"/>",
			"	</page>",
			"</xpath>"
		],
	},
	"Xpath - Replace": {
		"prefix": "Xpath - Replace",
		"body": [
			"<xpath expr=\"//field[@name='kasir_id']\" position=\"replace\">",
			"	<field name=\"kasir_id\" required=\"1\"/>",
			"</xpath>",
		],
	},
	"Xpath - Attributes": {
		"prefix": "Xpath - Attributes",
		"body": [
			"<xpath expr=\"//field[@name='is_aktif']\" position=\"attributes\">",
			"	<attribute name=\"required\">1</attribute>",
			"</xpath>"
		],
	},
	"Xpath - Attributes Group": {
		"prefix": "Xpath - Attributes Group",
		"body": [
			"<$0xpath expr=\"//field[@name='${1:is_aktif}']\" position=\"attributes\">",
			"	<attribute name=\"groups\">cafe_security.group_cafe_manager</attribute>",
			"</xpath>"
		],
	},
	"Xpath (Complex)": {
		"prefix": "Xpath Complex",
		"body": [
			"<xpath expr=\"/form/sheet/notebook/page/[2]\" position=\"inside\">",
			"	<field name=\"line_ids\"/>",
			"</xpath>",
			"<xpath expr=\"/form/sheet/notebook/page[1]/field[@name='order_line']/tree/field[@name='discount']\" position=\"after\">",
			"	<field name=\"kasir_id\"/>",
			"</xpath>"
		]
	},
	"New - Nomenclature sequence": {
		"prefix": "New - Nomenclature sequence",
		"body": [
			"<?xml version=\"1.0\" encoding=\"utf-8\"?>",
			"<odoo>",
			"	<data noupdate=\"0\"> <!-- Tips: File ini tidak di-baca ketika -u -->",
			"		<record model=\"ir.sequence\" id=\"${1:nama_model}_sequence\" >",
			"			<field name=\"name\">${3:Nama Model}</field>",
