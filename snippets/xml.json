	"Search View": {
		"prefix": "Search View",
		"body": [
			"<!-- Search View -->",
			"<record model=\"ir.ui.view\" id=\"${1:nama_model}_search\">",
			"	<field name=\"name\">${2:nama.model}.search</field>",
			"	<field name=\"model\">${2:nama.model}</field>",
			"	<field name=\"arch\" type=\"xml\">",
			"		<search>",
			"			<field name=\"name\"/>",
			"			<field name=\"partner_id\" filter_domain=\"[('partner_id','child_of',self)]\"/>",
			"			<filter name=\"fl_closed\" string=\"Closed\" domain=\"[('state','=', 'close')]\" help=\"Assets in closed state\"/>",
			"			<group>",
			"				<filter name=\"gr_day\" string=\"Day\" context=\"{'group_by':'date_paid:day'}\"/>",
			"				<filter name=\"gr_pay\" string=\"Method\" context=\"{'group_by':'pay_method'}\"/>",
			"			</group>",
			"		</search>",
			"	</field>",
			"</record>",
		],
	},
