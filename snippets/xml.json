	"Inherit Form": {
		"prefix": "Inherit Form ",
		"body": [
			"<?xml version=\"1.0\" encoding=\"utf-8\"?>",
			"<odoo>",
			"	<data>",
			"		<record model=\"ir.ui.view\" id=\"${2:nama_model_form}\">",
			"			<field name=\"name\">${1:nama.model}.form</field>",
			"			<field name=\"model\">${1:nama.model}</field>",
			"			<field name=\"inherit_id\" ref=\"${3:sale.view_order_form}\"/>",
			"			<field name=\"arch\" type=\"xml\">",
			"				<xpath expr=\"//field[@name='payment_term_id']\" position=\"after\">",
			"					<field name=\"voucher_id\" />",
			"				</xpath>",
			"			</field>",
			"		</record>",
			"	</data>",
			"</odoo>"
		],
	},
	"Inherit Tree": {
		"prefix": "Inherit Tree ",
		"body": [
			"<?xml version=\"1.0\" encoding=\"utf-8\"?>",
			"<odoo>",
			"	<data>",
			"		<record model=\"ir.ui.view\" id=\"${2:nama_model_tree}\">",
			"			<field name=\"name\">${1:nama.model}.tree</field>",
			"			<field name=\"model\">${1:nama.model}</field>",
			"			<field name=\"inherit_id\" ref=\"${3:sale.view_order_tree}\"/>",
			"			<field name=\"arch\" type=\"xml\">",
			"				<xpath expr=\"//field[@name='partner_id']\" position=\"after\">",
