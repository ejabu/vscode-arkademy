{
	"New - Security and Access Role": {
		"prefix": "New - Security and Access Role",
		"body": [
			"$0<?xml version=\"1.0\" encoding=\"utf-8\"?>",
			"<odoo>",
			"	<data>",
			"		<record model=\"ir.module.category\" id=\"module_${1:cafe}\">",
			"			<field name=\"name\">Cafe</field>",
			"			<field name=\"sequence\">1</field>",
			"		</record>",
			"",
			"		<record model=\"res.groups\" id=\"group_${1:cafe}_user\">",
			"			<field name=\"name\">Pegawai</field>",
			"			<field name=\"category_id\" ref=\"module_${1:cafe}\"/>",
			"			<field name=\"implied_ids\" eval=\"[(4, ref('base.group_user'))]\"/>",
			"		</record>",
			"",
			"		<record model=\"res.groups\" id=\"group_${1:cafe}_manager\">",
			"			<field name=\"name\">Manager</field>",
			"			<field name=\"category_id\" ref=\"module_${1:cafe}\"/>",
			"			<field name=\"implied_ids\" eval=\"[(4, ref('${2:nama_folder}.group_${1:cafe}_user'))]\"/>",
			"		</record>",
			"	</data>"
			"</odoo>"
		]
	},
	"New - Record Role": {
		"prefix": "New - Record Role",
		"body": [
			"<record model=\"ir.rule\" id=\"${1:rule_produk_non_aktif}\">",
			"	<field name=\"name\">${2:Produk Non Aktif}</field>",
			"	<field name=\"model_id\" ref=\"cafe.model_cafe_produk\"/>",
			"	<field name=\"domain_force\">[('is_aktif', '=', False)]</field>",
			"	<field name=\"groups\" eval=\"[(4, ref('cafe_security.group_cafe_user'))]\"/>",
			"</record>"
		]
	},
	"New - Class 1": {
		"prefix": "New - View (Standard)",
		"body": [
			"$0<?xml version=\"1.0\" encoding=\"UTF-8\"?>",
			"<odoo>",
			"	<data>",
			"		<!-- List View -->",
			"		<record model=\"ir.ui.view\" id=\"${1:nama_model}_tree\">",
			"			<field name=\"name\">${2:nama.model}.tree</field>",
			"			<field name=\"model\">${2:nama.model}</field>",
			"			<field name=\"arch\" type=\"xml\">",
			"				<tree>",
			"					<field name=\"name\"/>",
			"				</tree>",
			"			</field>",
			"		</record>",
			"		",
			"		<!-- Form View -->",
			"		<record model=\"ir.ui.view\" id=\"${1:nama_model}_form\">",
			"			<field name=\"name\">${2:nama.model}.form</field>",
			"			<field name=\"model\">${2:nama.model}</field>",
			"			<field name=\"arch\" type=\"xml\">",
			"				<form>",
			"					<header>",
			"					</header>",
			"					<sheet>",
			"						<group>",
			"							<group>",
			"								<field name=\"name\"/>",
			"							</group>",
			"							<group>",
			"							</group>",
			"						</group>",
			"					</sheet>",
			"				</form>",
			"			</field>",
			"		</record>",
			"",
			"		<record model=\"ir.actions.act_window\" id=\"${1:nama_model}_action\">",
			"			<field name=\"name\">${3:Nama Model}</field>",
			"			<field name=\"res_model\">${2:nama.model}</field>",
			"			<field name=\"view_type\">form</field>",
			"			<field name=\"view_mode\">tree,form</field>",
			"		</record>",
			"",
			"		<menuitem ",
			"			id=\"main_${1:nama_model}\"",
			"			name=\"${3:Nama Model}\"",
			"		/>",
			"",
			"		<menuitem ",
			"			id=\"menu_${1:nama_model}\"",
			"			name=\"${3:Nama Model}\"",
			"			parent=\"main_${1:nama_model}\"",
			"		/>",
			"",
			"		<menuitem ",
			"			id=\"submenu_${1:nama_model}\"",
			"			name=\"${3:Nama Model}\"",
			"			parent=\"menu_${1:nama_model}\"",
			"",
			"			action=\"${1:nama_model}_action\"",
			"		/>",
			"",
			"	</data>",
			"</odoo>"
		]
	},
