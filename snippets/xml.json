	"Xpath - After": {
		"prefix": "Xpath - After",
		"body": [
			"<xpath expr=\"//field[@name='${1:date_order}']\" position=\"${2:after}\">",
			"	<field name=\"${3:note}\"/>",
			"</xpath>",
		],
	},
	"Xpath - Before": {
		"prefix": "Xpath - Before",
		"body": [
			"<xpath expr=\"//field[@name='${1:date_order}']\" position=\"${2:before}\">",
			"	<field name=\"${3:note}\"/>",
