	"Inherit Form": {
		"prefix": "Inherit Form",
		"body": [
			"<?xml version=\"1.0\" encoding=\"utf-8\"?>",
			"<odoo>",
			"	<data>",
			"		<record model=\"ir.ui.view\" id=\"${1:nama_model}_form\">",
			"			<field name=\"name\">${2:nama.model}.form</field>",
			"			<field name=\"model\">${2:nama.model}</field>",
			"			<field name=\"inherit_id\" ref=\"${3:nama_modul}.${1:nama_model}_form\"/>",
			"			<field name=\"arch\" type=\"xml\">",
			"				$0<xpath expr=\"//field[@name='${4:date_order}']\" position=\"before\">",
			"					<field name=\"${5:note}\"/>",
			"				</xpath>",
			"			</field>",
			"		</record>",
			"	</data>",
			"</odoo>"
		],
	},
