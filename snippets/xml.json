	"Search View (Basic)": {
		"prefix": "Search View (Basic)",
		"body": [
			"<!-- Search View -->",
			"<record model=\"ir.ui.view\" id=\"${1:nama_model}_search\">",
			"	<field name=\"name\">${2:nama.model}.search</field>",
			"	<field name=\"model\">${2:nama.model}</field>",
			"	<field name=\"arch\" type=\"xml\">",
			"		<search>",
			"			<field name=\"name\"/>",
			"			<field name=\"pay_method\"/>",
			"			<filter name=\"fl_draft\" string=\"Draft\" domain=\"[('state','=', 'draft')]\"/>",
			"			<group>",
			"				<filter name=\"gr_pay\" string=\"Bayar Via\" context=\"{'group_by':'pay_method'}\"/>",
			"			</group>",
			"		</search>",
			"	</field>",
			"</record>",
		],
	},
	"Search View (Complex)": {
		"prefix": "Search View (Complex)",
		"body": [
			"<!-- Search View -->",
			"<record model=\"ir.ui.view\" id=\"${1:nama_model}_search\">",
			"	<field name=\"name\">${2:nama.model}.search</field>",
			"	<field name=\"model\">${2:nama.model}</field>",
			"	<field name=\"arch\" type=\"xml\">",
			"		<search>",
			"			<field name=\"name\"/>",
			"			<field name=\"pay_method\"/>",
			"			<field name=\"partner_id\" filter_domain=\"[('partner_id','child_of',self)]\"/>",
			"			<filter name=\"fl_closed\" string=\"Closed\" domain=\"[('state','=', 'close')]\" help=\"Assets in closed state\"/>",
			"			<group>",
			"				<filter name=\"gr_day\" string=\"Day\" context=\"{'group_by':'date_paid:day'}\"/>",
			"				<filter name=\"gr_pay\" string=\"Method\" context=\"{'group_by':'pay_method'}\"/>",
			"			</group>",
			"		</search>",
			"	</field>",
			"</record>",
		],
	},
	"Search View (Complex Datetime)": {
		"prefix": "Search View (Complex Datetime)",
		"body": [
			"<search string=\"Invoices Analysis\">",
            "   <filter string=\"This Year\" name=\"thisyear\"",
            "        domain=\"['|', ('date', '=', False), '&amp;',('date','&lt;=', time.strftime('%%Y-12-31')),('date','&gt;=',time.strftime('%%Y-01-01'))]\"/>",
            "    <filter string=\"This Month\" name=\"this_month\"",
            "        domain=\"[('date', '&gt;=', context_today().strftime('%%Y-%%m-01'))]\"/>",
            "    <filter string=\"Last Month\" name=\"last_month\"",
            "        domain=\"['&amp;', ('date', '&gt;=', (context_today() - relativedelta(months=1)).strftime('%%Y-%%m-01')), ('date', '&lt;', context_today().strftime('%%Y-%%m-01'))]\"/>",
            "</search>",
		],
	},
