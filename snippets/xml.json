	"New - Print Page Format": {
		"prefix": "New - Print Page Format",
		"body": [
			"<?xml version=\"1.0\" encoding=\"utf-8\"?>",
			"<odoo>",
			"	<data>",
			"		<record model=\"report.paperformat\" id=\"${1:nama_ukuran_kertas}_format\">",
			"			<field name=\"name\">${2:nama ukuran kertas}</field>",
			"			<field name=\"default\" eval=\"False\" />",
			"			<!-- <field name=\"format\">a5</field> --> <!-- Tips : Bisa pilih ukuran yg sudah ada -->",
			"			<field name=\"format\">custom</field>",
			"			<field name=\"page_width\">50</field>",
			"			<field name=\"page_height\">500</field>",
			"			<field name=\"orientation\">Landscape</field>",
			"			<field name=\"margin_top\">10</field>",
			"			<field name=\"margin_bottom\">20</field>",
			"			<field name=\"margin_left\">7</field>",
			"			<field name=\"margin_right\">7</field>",
			"			<field name=\"header_line\" eval=\"False\" />",
			"			<field name=\"header_spacing\">35</field> <!-- Tips : Hide unnecessary header -->",
			"		</record>",
			"	</data>",
			"</odoo>"
		],
	},
	"New - Print": {
		"prefix": "New - Print",
		"body": [
			"<?xml version=\"1.0\" encoding=\"utf-8\"?>",
			"<odoo>",
			"	<data>",
			"		<report",
			"			id=\"${1:nama_model}_hardcopy\"",
			"			model=\"${2:nama.model}\"",
			"			string=\"${3:Nama Model}\"",
			"			name=\"${4:nama_modul}.${1:nama_model}_print_template\"",
			"			report_type=\"qweb-pdf\"",
			"			print_report_name=\"'Nama File - %s' % (object.name).replace('/','')\"",
			"			paperformat=\"base.paperformat_us\"/>",
			"		/>",
			"		<template id=\"${1:nama_model}_print_template\">",
			"			<t t-call=\"web.html_container\">",
			"				<t t-foreach=\"docs\" t-as=\"doc\">",
			"					<t t-call=\"web.internal_layout\">",
			"						<style>",
			"							.text-center{",
			"								text-align: center;",
			"							}",
			"						</style>",
			"						<div class=\"page\">",
			"							<div class=\"row text-center mb32\">",
			"								<h3>",
			"									Voucher <span t-esc=\"doc.name\" />",
			"								</h3>",
			"							</div>",
			"							<div class=\"row text-center mb32\">",
			"								Name <span style=\"font-weight:bold\" t-esc=\"doc.name\" />",
			"								State <span style=\"font-weight:bold\" t-esc=\"doc.state\" />",
			"								SO <span style=\"font-weight:bold\" t-esc=\"doc.sale_order_terkait\" />",
			"							</div>",
			"						</div>",
			"					</t>",
			"				</t>",
			"			</t>",
			"		</template>",
			"	</data>",
			"</odoo>"
		]
	}
}
