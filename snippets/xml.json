			"				</xpath>",
			"			</field>",
			"		</record>",
			"	</data>",
			"</odoo>"
		],
	},
	"Xpath - After": {
		"prefix": "Xpath - After",
		"body": [
			"<xpath expr=\"//field[@name='payment_term_id']\" position=\"after\">",
			"	<field name=\"kasir_id\"/>",
			"</xpath>",
		],
	},
	"Xpath - Before": {
		"prefix": "Xpath - Before",
		"body": [
			"<xpath expr=\"//field[@name='payment_term_id']\" position=\"before\">",
			"	<field name=\"kasir_id\"/>",
			"</xpath>",
		],
	},
	"Xpath - Inside": {
		"prefix": "Xpath - Inside",
		"body": [
			"<xpath expr=\"//notebook\" position=\"inside\">",
			"	<page string=\"Other Info\">",
			"		<field name=\"is_part_time\"/>",
			"	</page>",
			"</xpath>"
		],
	},
	"Xpath - Replace": {
		"prefix": "Xpath - Replace",
		"body": [
			"<xpath expr=\"//field[@name='kasir_id']\" position=\"replace\">",
			"	<field name=\"kasir_id\" required=\"1\"/>",
			"</xpath>",
		],
	},
	"Xpath - Attributes": {
		"prefix": "Xpath - Attributes",
		"body": [
			"<xpath expr=\"//field[@name='kasir_id']\" position=\"attributes\">",
			"	<attribute name=\"required\">1</attribute>",
			"</xpath>"
		],
	},
	"Xpath (Complex)": {
		"prefix": "Xpath Complex",
		"body": [
			"<xpath expr=\"/form/sheet/notebook/page/[2]\" position=\"inside\">",
			"	<field name=\"line_ids\"/>",
			"</xpath>",
			"<xpath expr=\"/form/sheet/notebook/page[1]/field[@name='order_line']/tree/field[@name='discount']\" position=\"after\">",
			"	<field name=\"kasir_id\"/>",
			"</xpath>"
		]
	},
	"New - Sequence / Nomenclature": {
		"prefix": "New - Sequence / Nomenclature",
		"body": [
			"<?xml version=\"1.0\" encoding=\"utf-8\"?>",
			"<odoo>",
			"	<data noupdate=\"1\"> <!-- Tips: File ini tidak di-baca ketika -u -->",
			"		<record model=\"ir.sequence\" id=\"${1:nama_model}_sequence\" >",
			"			<field name=\"name\">${3:Nama Model}</field>",
			"			<field name=\"code\">${2:nama.model}.sequence</field>",
			"			<field name=\"prefix\">PR/%(y)s/</field>",
			"			<field name=\"padding\">5</field>",
			"			<field name=\"company_id\" eval=\"False\" />",
			"		</record>",
			"	</data>",
			"</odoo>",
		],
	},
