	"New - Print (Complex)": {
		"prefix": "New - Print (Complex)",
		"body": [
			"<?xml version=\"1.0\" encoding=\"utf-8\"?>",
			"<odoo>",
			"	<data>",
			"		<report",
			"			id=\"${1:nama_model}_print\"",
			"			model=\"${2:nama.model}\"",
			"			string=\"${3:Nama Model}\"",
			"			name=\"${4:nama_modul_ini}.${1:nama_model}_template\"",
			"			report_type=\"qweb-pdf\"",
			"			print_report_name=\"'${3:Nama Model} - %s' % (object.name)\"",
			"			paperformat=\"base.paperformat_us\"/>",
			"		/>",
			"		$0<template id=\"cafe_order_template\">",
			"			<t t-call=\"web.html_container\">",
			"				<t t-foreach=\"docs\" t-as=\"doc\">",
			"					<t t-call=\"web.internal_layout\">",
			"						<style>",
			"							table {",
			"								border-collapse: collapse;",
			"								width: 100%;",
			"							}",
			"							table td, table th {",
			"								border: 1px solid #ddd;",
			"								padding: 8px;",
			"							}",
			"							table tr:nth-child(even){background-color: #f2f2f2;}",
			"						</style>",
			"						Nota : <span t-esc=\"doc.name\"/>",
			"						Status : <span t-esc=\"doc.state\"/>",
			"						<div>",
			"							<table>",
			"								<tr>",
			"									<th>",
			"										Produk",
			"									</th>",
			"									<th>",
			"										Qty",
			"									</th>",
			"									<th>",
			"										Price",
			"									</th>",
			"									<th>",
			"										Price Total",
			"									</th>",
			"								</tr>",
			"								<t t-foreach=\"doc.line_ids\" t-as=\"line\">",
			"									<tr>",
			"										<td>",
			"											<span t-esc=\"line.produk_id.name\"/>",
			"										</td>",
			"										<td>",
			"											<span t-esc=\"line.quantity\"/>",
			"										</td>",
			"										<td>",
			"											<span t-esc=\"line.price\"/>",
			"										</td>",
			"										<td>",
			"											<span t-esc=\"line.price_total\"/>",
			"										</td>",
			"									</tr>",
			"								</t>",
			"							</table>",
			"						</div>",
			"					</t>",
			"				</t>",
			"			</t>",
			"		</template>",
			"	</data>",
			"</odoo>"
		]
